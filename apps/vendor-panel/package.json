    "@radix-ui/react-dismissable-layer": "^1.1.4",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.0",
    "@talkjs/react": "^0.1.11",
    "@tanstack/react-query": "catalog:",
